{
  "name": "server",
  "description": "standalone, back-end Express web server for UCI ML Repository",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "./dist/trpc-server",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "build": "ts-node ./scripts/esbuild.ts && pkg .",
    "format": "prettier --write .",
    "lint": "eslint --fix ."
  },
  "bin": "dist/bundle.js",
  "pkg": {
    "targets": [
      "node16-linux-x64"
    ],
    "assets": [
      "dist/schema.prisma"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.3",
    "@types/nodemailer": "^6.4.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "esbuild": "^0.15.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.19",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@trpc/server": "^9.27.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.8",
    "zod": "^3.18.0"
  }
}
